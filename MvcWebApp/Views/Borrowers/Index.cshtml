@model IEnumerable<Data.Borrower>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p> 
    <!-- Large modal -->
    <button type="button" class="btn btn-primary" data-toggle="modal" data-target=".bs-example-modal-lg" onclick="Register()">Register</button>
</p>
<div class="container">
    <br />
    <div style="margin:0 auto;">
        <table id="borrowers" class="table table-striped table-bordered dt-responsive nowrap" width="100%" cellspacing="0">
            <thead>
                <tr>
                    <th>Password</th>
                    <th>First Name</th>
                    <th>Last Name</th>
                    <th>Gender</th>
                    <th>Login</th>
                    <th>Edit</th>
                    <th>Delete</th>
                </tr>
            </thead>
        </table>
    </div>
</div>

<script src="//code.jquery.com/jquery-1.11.2.min.js" type="text/javascript"></script>
<script src="~/js/Borrowers/table.js"></script>
 
<div class="modal fade bs-example-modal-lg" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <div id="dynamicContentContainer"></div> 
        </div>
    </div>
</div>
 